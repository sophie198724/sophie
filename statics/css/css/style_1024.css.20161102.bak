	/* Nav */

.nav{
	font-size:14px;
	line-height:20px;
	background:url(../../images/images/nav_bg.png) repeat-x;
	width:100%;
	text-align:center;
	position:relative;
	height:68px;
	display:block!important;
	
	/* remove absolute header start */
	position:absolute;
	top:95px;
	z-index:9999;
	/* remove absolute header end */
	
	font-weight:400;
}
.nav .menu{
	text-transform:uppercase;
	width:1200px;
	height:68px;
	margin:auto;
	padding-left:61px;
}
.nav .menu > li{
	width:160px;
	cursor:pointer;
	text-decoration:none;
	color:#fff;
	float:left;
	height:68px;
	background:url(../../images/images/nav_bg.png) repeat-x;
	background-color:#28166f;
}
.nav .menu > li span{
	display:block;
	padding:23px 0px 20px 0px;
	text-align:center;
	height: 68px;
}
.nav .menu > li > a{
	color:#FFF;
	width:147px;
	display:block;
	padding:23px 0px 20px 0px;
	text-align:center;
	text-decoration:none;
	background-repeat:no-repeat;
	background-position:left;
}

.nav .menu > li.hover, .nav .menu > li.selected{
	color:#333;
	background-color:#EFEDCB;
	background-image:url(../../images/images/nav_arrow_hover1.png);
}

.nav .menu > li.hover
{
	opacity:0.1;
}
.nav .submenuWrapper{
	position:absolute;
	width:100%;
	left:0px;
	top:68px;
	background:url(../../images/images/nav2_bg_03.png) bottom repeat-x;
	z-index:9999;
	display:none;
	padding-bottom: 10px;
}
.nav .submenuContainer{
	text-align:left;	
	width:960px;
	margin:auto;
	padding:25px 0 0 117px;
}

.nav .submenu{
	
	vertical-align:top;
	width:auto;
	margin-right:80px;
	line-height:17px;
	float:left;
}
.nav .submenu li{margin-bottom:18px;}
.nav .submenu a{
	color:#333333;
	text-decoration:none;
	text-transform:none;
}
.nav .submenu a:hover{text-decoration:underline;}

.nav_fixed
{
	position: fixed;
	top:-1px;
	left:0;
	
}

.nav_static
{
	position: absolute;
	top:95px;
	left:0;
}

.wrapperNavOn
{
	margin-top:100px;
}

.wrapperNavOff
{
	margin-top:0;
}


/* Desktop break */
.desktopBr
{
	display:block;
}
.desktopBr:after
{
	content:"\00a0";
}

/* horizontal scroll Nav */
#leftScrollBtn, #rightScrollBtn
{
	width:50px;
	height:92px;
	background-image:url(../../images/images/scrollbtn_bg.png);
	background-repeat:no-repeat;
	position:absolute;
	display:block;
}
.left-nav .content li a{
	color: #000 !important;
}
.left-nav .content li a.on{
	color: #fff !important;
}


#yearNavMobile
{
	display:none;
}

.mobilePointer
{
	display:none;
}

.navigation
{
	width:auto!important;
}

/* Home */
.homeBanner
{
	display:none;
}

#desktopBanner
{
	display:block;
	background-color:#EFEFCE;
}

/* set dlBlock title height */
.title1line .dlBlock .title
{
	height:35px;
}

.title2line .dlBlock .title
{
	height:50px;
}

.title3line .dlBlock .title
{
	height:65px;
}

#about_board
{
	padding-left:10px;
}

#about_senior
{
	padding-bottom:100px;	
}
.left-nav .content li a:hover{
	background: #28166f;
	color: #fff !important;
};